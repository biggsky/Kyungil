<body>
    <h2>리스트 페이지</h2>
    <a href="/insert">추가 페이지로 이동</a>
    <table>
        <tr>
            <td><a href="">삭제</a> </td>
            <td>수정</td>
            <td>아이디</td>
            <td>이름</td>
            <td>전화번호</td>
            <td>시리즈</td>
        </tr>
        <% if(data){ %>

            <% data.forEach((el,index) => { %>
            <tr>
                <td><a href="/delete/<%= el.id %>"> 삭제</a></td>
                <td><a href="/edit/<%= el.id %>">수정</a></td>
                <td><%= el.id %></td>
                <td><%= el.name %></td>
                <td><%= el.number %></td>
                <td><%= el.series %></td>
            </tr>
            <% }); %>

        <% }else{ %>

        <% } %>
    </table>
</body>
